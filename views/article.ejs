<%-include("header")-%>
<div class="content">
    <h1>Selected articles</h1>
    <table id="catalog">
        <thead>
            <tr>
                <th>Name</th>
                <th scope="col">Category</th>
                <th scope="col">Price</th>
                <th scope="col">Stock</th>
                <th scope="col">Critical Stock</th>
            </tr>
        </thead>
        <tbody>
            <%  %>
                <tr>
                    <th scope="row">
                        <%= article.name %>
                    </th>

                    <td>
                        <%= article.category %>
                    </td>
                    <td>
                        <%= article.price %>AED
                    </td>
                    <td>
                        <%= article.stock %>
                    </td>
                    <td>
                        <%= article.critical_stock %>
                    </td>
                </tr>
                <% %>
        </tbody>
    </table>
</div>
<br>
<%-include("footer")-%>